{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/callback/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/reportes/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/resenas/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/resenas/?$",
			"dest": "_render"
		},
		{
			"src": "^/dictado/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/materia/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/nivel/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/nueva-resena/?$",
			"dest": "_render"
		},
		{
			"src": "^/profesor/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/sobre/?$",
			"dest": "_render"
		},
		{
			"src": "^/terminos/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}